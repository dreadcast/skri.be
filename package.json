{
	"name": "writenode",
	"version": "0.2.1",
	"homepage": "http://converge.fr",
	"description": "Simple static blog generator",
	"author": {
		"name": "Raphael Gully",
		"website": "http://dreadcast.com"
	},

	"readmeFilename": "Readme.md",
	"homepage": "https://github.com/dreadcast/writenode",

	"keywords": [
		"website",
		"blog",
		"templating",
		"generator",
		"static"
	],

	"repository": {
		"type": "git",
		"url": "git@bitbucket.org:dreadcast/writenode.git"
	},
	"bugs": {
		"url": "https://github.com/dreadcast/writenode/issues"
	},

	"dependencies": {
		"backbone": "1.2.3",
		"json2yaml": "1.0.3",

		"bluebird": "3.0.5",
		"uglify-js": "2.5.0",

		"less": "2.5.3",
		"less-plugin-autoprefix": "1.5.1",

		"swig": "1.4.2",
		"nunjucks": "2.1.0",
		"html-minifier": "1.0.0",
		"node-useref": "0.3.15",

		"marked": "0.3.5",
		"front-matter": "2.0.0",

		"express": "4.13.3",

		"fs-extra": "0.26.2",
		"fs-recurse": "0.0.6",

		"request-promise": "1.0.2",

		"imageinfo": "1.0.4",

		"cheerio": "0.19.0",
		"superscore.string": "latest",
		"rss": "1.2.0",
		"prompt": "0.2.13",
		"open": "0.0.5",
		"moment": "2.9.0",

		"anymatch": "1.3.0",
		"chokidar": "1.2.0",
		"lowerdash": "0.4.0-draft-3",
		"lodash": "3.10.1",
		"jimp": "0.2.13",

		"babel": "5.8.29",

		"babel-core": "latest",
		"babel-preset-stage-0": "latest",
		"babel-preset-es2015": "latest",
		"babel-plugin-transform-class-properties": "latest",
		"babel-plugin-transform-object-rest-spread": "latest"
	},

	"devDependencies": {
		"nodemon": ">=1.0.14"
	},

	"babel": {
		"stage": 0
	},
	"babel6": {
		"plugins": ["transform-class-properties", "transform-object-rest-spread"],
		"presets": ["stage-0", "es2015"]
	},

	"main": "./index.js",

	"scripts": {
		"start": "nodemon index.js"
	}
